@model UpdateGalleryDTO
@{
    ViewData["Title"] = "UpdateGallery";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<form method="post">
    <input type="hidden" asp-for="Id" />
    <div class="card-body">
        <div class="form-group">
            <label for="GalleryImageURL">Başlığı Giriniz</label>
            <input type="text" class="form-control  text-dark bg-light" asp-for="GalleryImageURL" id="GalleryImageURL">

        </div>
        <br />
        <button class="btn btn-outline-primary">Galeriye Yeni Görsel Bilgisini Ekle</button>
    </div>
</form>


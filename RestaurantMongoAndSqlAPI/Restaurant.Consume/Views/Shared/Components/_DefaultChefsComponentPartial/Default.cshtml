@model List<ResultTeamDTO>
<section id="chefs" class="chefs section">

    <!-- Section Title -->
    <div class="container section-title" data-aos="fade-up">
        <h2>Takım Arkadaşlarımız</h2>
        <p>Sizler İçin Hazırlık Yapan Takım Arkadaşlarımız</p>
    </div><!-- End Section Title -->

    <div class="container">
          
        <div class="row gy-4">
            @foreach(var item in Model){
            <div class="col-lg-4" data-aos="fade-up" data-aos-delay="100">
              
                <div class="member">
                    <img src="@item.TeamImageURL" class="img-fluid" alt="">
                    <div class="member-info">
                        <div class="member-info-content">
                            <h4>@item.TeamFullName</h4>
                            <span>@item.TeamTitle</span>
                        </div>
                        <div class="social">
                            <a href="@item.TeamSocialMedia1"><i class="bi bi-twitter-x"></i></a>
                            <a href="@item.TeamSocialMedia2"><i class="bi bi-facebook"></i></a>
                            <a href="@item.TeamSocialMedia3"><i class="bi bi-instagram"></i></a>
                            <a href="@item.TeamSocialMedia4"><i class="bi bi-linkedin"></i></a>
                        </div>
                    </div>
                </div>
               
            </div><!-- End Team Member -->
                }
        </div>
        
    </div>

</section>
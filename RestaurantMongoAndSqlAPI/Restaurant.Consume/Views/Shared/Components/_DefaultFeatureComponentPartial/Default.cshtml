@model List<ResultFeatureDTO>
<section id="hero" class="hero section dark-background">

    <img src="~/restaurantly-1.0.0/assets/img/hero-bg.jpg" alt="" data-aos="fade-in">

    <div class="container">
        <div class="row">
            @foreach(var item in Model){
            <div class="col-lg-8 d-flex flex-column align-items-center align-items-lg-start">
                <h2 data-aos="fade-up" data-aos-delay="100">Hoşgeldiniz <span>@item.FeatureTitle</span></h2>
                <p data-aos="fade-up" data-aos-delay="200">@item.FeatureSubTitle</p>
                <div class="d-flex mt-4" data-aos="fade-up" data-aos-delay="300">
                    <a href="#menu" class="cta-btn">Menümüz</a>
                    <a href="#book-a-table" class="cta-btn">Rezervasyon Yapın</a>
                </div>
            </div>
            }
        </div>
    </div>

</section>